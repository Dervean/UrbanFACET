<!-- NEW VISUAL ANALYTICS LOGIC -->
<div class="sixteen wide column">
    <h5><i class="right angle icon divider"></i>Analyzed Object</h5>
    <div class="ui form">
        <div class="inline fields">
            <button id="claExpandBtn" class="ui mini secondary button" v-on:click='claExpandDisplay'>Expand</button>

            <div class="ui mini compact menu">
                <div id="vcclaDropdown" class="ui simple dropdown item">
                    <span>{{ selections.vcclaName }}</span>
                    <i class="dropdown icon"></i>
                    <div class="menu">
                        <div v-for='item in settings.classes' class="item" v-on:click='changeSelectCla(item)'>{{ item }}</div>
                    </div>
                </div>
            </div>
            <div class="ui mini compact menu">
                <div class="ui simple dropdown item">
                    <span>{{ selections.vctimeName }}</span>
                    <i class="dropdown icon"></i>
                    <div class="menu">
                        <div v-for='item in settings.daytypes' class="item">
                            <i class="dropdown icon"></i>
                            <span>{{ item.name }}</span>
                            <div class="menu">
                                <div v-for="tpitem in item.timeperiods" class="item" v-on:click='changevcTime(item.name, tpitem.name, item.val, tpitem.val)'>{{ tpitem.name }}</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
           
            <button id="vcBtn" class="ui mini secondary button" :class="{ 'loading': states.vcquery }" v-on:click='vcQuery("basic")'>Filter</button>
        </div>
    </div>
</div>