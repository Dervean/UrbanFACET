<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>
        <%= title %> | ISCAS-VIS
    </title>
    <meta name="description" content="">
    <meta name="keywords" content="">
    <link href="/favicon.ico" rel="shortcut icon">
    <style type="text/css" rel="stylesheet">
    @import url('/css/leaflet.css');
    @import url('/css/semantic.css');
    @import url('/css/pages/<%= name %>.css');
    </style>
</head>

<body>
    <div id="titlemenu" class="ui inverted menu">
        <div class="header item"><h2>Urban Mobility Map: Information-theoretic visual analysis of big user check-in data</h2></div>
    </div>

    <div class="ui row" id="main">    
        <div id="userpanel" class="ui segment">
            <% include vis-x/region %>
        </div>
        <div id="mappanel" class="ui segment">
            <div id="map0" class="onemap vamap ui segment">
                <div id="maplegend0">
                    <svg id='gridmaplegend0' height='50'></svg>
                    <div id="contourmaplegend0" ></div>
                </div>

                <div class="ui dimmer">
                    <div class="ui medium text loader">Loading Data</div>
                </div>
            </div>

            <!-- <div id="map1" class="twomap vamap ui segment"></div> -->
        </div>
            
    </div>
    
    <script src="/dist/<%= name %>.js"></script>
</body>

</html>
